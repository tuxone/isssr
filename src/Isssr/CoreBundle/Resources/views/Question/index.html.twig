<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Question</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('question_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.question }}</td>
            <td>{{ entity.status }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
